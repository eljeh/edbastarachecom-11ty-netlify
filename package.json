{
  "name": "edbastarache.com",
  "version": "",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "watch": "npx eleventy --watch",
    "watch:sass": "sass  --no-source-map --watch src/_includes/assets/sass:dist/assets/styles",
    "watch:eleventy": "eleventy --serve",
    "serve": "npx eleventy --serve",
    "debug": "DEBUG=* npx eleventy",
    "build:sass": "sass  --no-source-map src/_includes/assets/sass:dist/assets/styles",
    "build:eleventy": "eleventy",
    "postbuild": "postcss _dist/assets/styles/*.css -u autoprefixer cssnano -r --no-map",
    "build": "npm-run-all build:sass build:eleventy",
    "start": "npm-run-all build:sass --parallel watch:*"
  },
  "author": {
    "name": "Ed Bastarache",
    "url": "https://edbastarache.com/"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-img": "^0.8.0",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eleventy-plugin-reading-time": "0.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8",
    "uglify-es": "^3.3.9",
    "luxon": "^1.25.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.3.0"
  },
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6"
  }
}
