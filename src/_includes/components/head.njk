<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <meta http-equiv="content-language" content="en-CA">
  <meta charset="utf-8">
  <link rel="dns-prefetch" href="https://identity.netlify.com">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="robots" content="index,follow">

	<!-- Site Content -->  
  <title>{{ renderData.title or title }}{%- if srenderData.title or title  %} | {% endif %}{{site.name}}{%- if site.tagline %} - {{site.tagline}}{%- endif %}</title>
  <meta name="description" content="{{ metaDescription or renderData.metaDescription or metadata.description }}">
  <link name="keywords" content="{{pageKeywords}}"> 

  <!-- theme -->
  <meta name="theme-color" content="{{site.themeColor}}">
  <meta name="apple-mobile-web-app-status-bar-style" content="{{site.themeColor}}">

  <!-- Icons -->
  <link rel="shortcut icon" href="{{site.favicon | url}}" type="image/x-icon">
  <link rel="apple-touch-icon" href="{{site.icon16 | url}}" sizes="16x16" type="image/png">
  <link rel="icon" href="{{site.icon192 | url}}" sizes="192x192" type="image/png">
  
  <!-- Google / Search Engine -->
  <meta itemprop="name" content="{{ renderData.title or title }}{%- if srenderData.title or title  %} | {% endif %}{{site.name}}{%- if site.tagline %} - {{site.tagline}}{%- endif %}">
  <meta itemprop="description" content="{{ metaDescription or renderData.metaDescription or metadata.description }}">
  <meta itemprop="image" content="{{site.thumbnail | url}}">
  <meta name="publisher" content="{{site.publisher}}">
  <meta name="google" content="notranslate">

  <!-- Open Graph / Facebook -->
  <meta property="og:url" content="{{site.url}}">
  <meta property="og:title" content="{{ renderData.title or title }}{%- if srenderData.title or title  %} | {% endif %}{{site.name}}{%- if site.tagline %} - {{site.tagline}}{%- endif %}"">
  <meta property="og:description" content="{{ metaDescription or renderData.metaDescription or metadata.description }}">
  <meta property="og:image" content="{{site.thumbnail | url}}">
  <meta property="og:type" content="website">
  <meta property="og:locale" content="en_CA">
  <meta property="og:locale:alternate" content="en_FR">

  <!-- Twitter -->
  <meta name="twitter:url" content="{{metadata.url | url}}">
  <meta name="twitter:title" content="{{ renderData.title or title }}{%- if srenderData.title or title  %} | {% endif %}{{site.name}}{%- if site.tagline %} - {{site.tagline}}{%- endif %}"">
  <meta name="twitter:description" content="{{ metaDescription or renderData.metaDescription or metadata.description }}">
  <meta name="twitter:image:src" content="{{site.thumbnail | url}}">
  
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Lato:wght@400;700;900&family=Raleway:wght@100;200;300;400&display=swap">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css">
   
  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  {# {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style> #}
  {# {% set js %}
    {% include "assets/scripts/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script> #}
  {# <link rel="stylesheet" href=".assets/styles/inline.css"> #}
  <link rel="stylesheet" href="../assets/styles/root-styles.css">
  <link rel="stylesheet" href="../assets/styles/{{pagestyle}}">
</head>
